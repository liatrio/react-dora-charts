{
  "name": "react-dora-charts",
  "version": "0.0.0",
  "description": "DORA Metric Charts for React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "./dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "clean": "rimraf dist",
    "test": "jest",
    "playwright": "playwright test --config=playwright/playwright.config.js",
    "playwright:show-report": "playwright show-report",
    "playwright:update-snapshots": "playwright test --config=playwright/playwright.config.js --update-snapshots",
    "playwright:docker:build": "docker build -t playwright-image -f playwright/playwright.dockerfile .",
    "playwright:docker:run": "docker run -it --rm --name playwright -v $(pwd):/app playwright-image",
    "playwright:docker:run:update-snapshots": "docker run -it --rm --name playwright -v $(pwd):/app playwright-image npm run playwright:update-snapshots",
    "storybook": "storybook dev -p 6006",
    "storybook-ci": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "prettier:check": "npx --yes prettier --check .",
    "prettier:fix": "npx --yes prettier --write .",
    "tsc:full": "tsc --skipLibCheck true --incremental false"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@playwright/test": "^1.46.1",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@spotify/prettier-config": "^15.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.1.3",
    "@storybook/cli": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-webpack5": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.2",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom-global": "^3.0.2",
    "react-datepicker": "^7.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-rebase": "^4.1.1",
    "storybook": "^8.1.3",
    "storybook-dark-mode": "^4.0.1",
    "ts-jest": "^29.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "react-tooltip": "^5.28.0",
    "recharts": "^2.12.7",
    "uuid": "^10.0.0"
  },
  "prettier": "@spotify/prettier-config"
}
